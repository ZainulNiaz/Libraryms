<!DOCTYPE html>
<html lang="en">
<head>
  <title>Clienthome</title>
</head>
<body>
    <div class="table-data">
<h2>Client Home</h2>
    <table border="1">
        <tr>
            <th>S.N</th>
            <th>Full Name</th>
            <th>Email Address</th>
            <th>Password</th>
        </tr>
        
        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <tr>
            <td><%=data.id %></td>
            <td><%=data.name %></td>
            <td><%=data.email %></td>
            <td><%=data.password %></td>

        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>